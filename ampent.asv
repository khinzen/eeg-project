function [result] = ampent(input, open)

[length, width] = size(input)

result = zeros(1,3);

for ii = [1:200:(length-200)]

    block = input([ii, ii+200], 66)
    amp = max(block) - min(block)
    ent = approximateEntropy(block)
    
    result = [result; amp ent]
end

result = result([2:end], :);
